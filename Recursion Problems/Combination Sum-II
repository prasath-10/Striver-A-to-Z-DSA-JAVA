class Solution {
        public void findsub(int index,int[] candidates,int target,List<Integer> ds,List<List<Integer>> ans)
    {
        if(target==0)
        {
            ans.add(new ArrayList<>(ds));
        }
        for(int i=index;i<candidates.length;i++)
        {
            if(i!=index && candidates[i]==candidates[i-1])
            {
                continue;
            }
            if(candidates[i]>target)
            {
                break;
            }
            ds.add(candidates[i]);
            findsub(i+1,candidates,target-candidates[i],ds,ans);
            ds.remove(ds.size()-1);
        }
    }
    public List<List<Integer>> combinationSum2(int[] candidates, int target) {
        Arrays.sort(candidates);
        List<List<Integer>> ans=new ArrayList<>();
        findsub(0,candidates,target,new ArrayList<>(),ans);
        return ans;
        
    }
}
